    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.2.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@types/aws-sdk": "^2.7.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
